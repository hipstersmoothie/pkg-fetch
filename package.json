{
  "name": "@jesec/pkg-fetch",
  "version": "2.7.0",
  "description": "Compiles and stores base binaries for pkg",
  "main": "lib-es5/index.js",
  "license": "MIT",
  "repository": "jesec/pkg-fetch",
  "bin": {
    "pkg-fetch": "lib-es5/bin.js"
  },
  "files": [
    "lib-es5/*.js",
    "patches/*",
    "places.json"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "4"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "byline": "^5.0.0",
    "chalk": "^4.1.0",
    "expand-template": "^2.0.3",
    "fs-extra": "^9.0.1",
    "minimist": "^1.2.5",
    "progress": "^2.0.3",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "semver": "^7.3.4",
    "unique-temp-dir": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0"
  },
  "scripts": {
    "babel": "babel lib --out-dir lib-es5",
    "bin": "node lib-es5/bin.js",
    "lint": "eslint .",
    "prepare": "npm run babel",
    "prepublishOnly": "eslint . ",
    "start": "npm run babel && node lib-es5/upload.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-prettier",
    "parser": "babel-eslint"
  }
}
